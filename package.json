{
  "name": "DN",
  "description": "Basic website setup",
  "contributors": [],
  "dependencies": {
    "@popperjs/core": "^2.5.4",
    "float-labels.js": "^3.3.9",
    "formbouncerjs": "^1.4.6",
    "gmap3": "^7.2.0",
    "gulp-cli": "^2.3.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "hotkeys-js": "^3.8.1",
    "jquery": "^3.5.1",
    "jquery-match-height": "^0.7.2",
    "jquery.browser": "^0.1.0",
    "js-cookie": "^2.2.1",
    "magnific-popup": "^1.1.0",
    "node-prune": "^1.0.2",
    "npm-watch": "^0.7.0",
    "swiper": "^6.3.5",
    "tailwindcss": "^1.9.6",
    "tippy.js": "v6.2.7",
    "typescript": "^4.0.5",
    "watch": "^1.0.2"
  },
  "main": "resources/js/node.js",
  "version": "1.1.6",
  "watch": {
    "crunch": {
      "patterns": [
        "public_html"
      ],
      "extensions": "html,php",
      "delay": 0
    }
  },
  "scripts": {
    "crunch": "cross-env NODE_ENV=production postcss node_modules/tailwindcss/dist/utilities.min.css -o public_html/resources/css/tailwind.css",
    "build:css": "tailwind build public_html/resources/css/tailwindsass.css -o public_html/resources/css/tailwind.css",
    "gulp": "gulp",
    "postinstall": "node-prune",
    "pro-gulp": "npm run production; gulp",
    "production": "node node_modules/cross-env/src/bin/cross-env.js NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "dev": "node node_modules/cross-env/src/bin/cross-env.js NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm-watch",
    "hot": "node node_modules/cross-env/src/bin/cross-env.js NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^3.0.0",
    "autoprefixer": "^10.0.1",
    "axios": "^0.19",
    "caniuse-lite": "^1.0.30001154",
    "cross-env": "^7.0",
    "cssnano": "^4.1.10",
    "del": "^6.0.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-minify": "^3.1.0",
    "gulp-postcss": "^9.0.0",
    "gulp-purgecss": "^3.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "laravel-mix": "^5.0.1",
    "postcss": "^8.1.4",
    "postcss-cli": "^8.2.0",
    "postcss-font-magician": "^3.0.0",
    "postcss-import": "^13.0.0",
    "postcss-nested": "^5.0.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-short": "^5.0.0",
    "postcss-simple-vars": "^6.0.1",
    "resolve-url-loader": "^3.1.0",
    "webpack-stream": "^6.1.0"
  },
  "_dependencies_": {
    "tooltipster": "^4.2.8",
    "@tannerhodges/match-height": "^1.1.1",
    "formbouncerjs": "^1.4.6",
    "slick-carousel": "^1.8.1",
    "graceful-fs": {
      "version": "4.2.2"
    },
    "jquery-validation": "^1.19.2",
    "js-cookie": "^2.2.1"
  },
  "options": {
    "config": {
      "tailwindjs": "./tailwind.config.js"
    },
    "paths": {
      "root": "./",
      "src": {
        "base": "./src",
        "css": "./src/css",
        "js": "./src/js",
        "img": "./src/img"
      },
      "dist": {
        "base": "./dist",
        "css": "./dist/css",
        "js": "./dist/js",
        "img": "./dist/img"
      },
      "build": {
        "base": "./build",
        "css": "./build/css",
        "js": "./build/js",
        "img": "./build/img"
      }
    }
  }
}
